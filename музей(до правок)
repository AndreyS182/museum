public class Artifact {
    int num;
    String culture;
    int year;
static int count = 0;
  public Artifact(int num){
      this.num = num;
  }
  public Artifact(int num, int year){
      this.num = num;
      this.year = year;

  }
  public Artifact(int num, int year, String culture) {
      this.num = num;
      this.year = year;
      this.culture = culture;
  }
      public static void main(String[] args){
      Artifact Artifact1 = new Artifact(182);
      Artifact Artifact2 = new Artifact(183, 5);
      Artifact Artifact3 = new Artifact(184, 7, "Aztecs");
      System.out.println("Артифакт номер " + Artifact1.num + ".");
      System.out.println("Артифакт номер " + Artifact2.num + ". Век - " + Artifact2.year + ".");
      System.out.println("Артифакт номер " + Artifact3.num + ". Век - " + Artifact3.year + ". Культура - " + Artifact3.culture + ".");
  }

}
