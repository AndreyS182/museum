public class Artifact {
    int num;
    String culture;
    int year;
static int count = 0;
  public Artifact(){
  count ++;
  this.num = 182;

  }
  public Artifact(int num, int year){
      this.num = 183;
      this.year = 5;

  }
  public Artifact(int num, int year, String culture) {
      this.num = 184;
      this.year = 4;
      this.culture = "Aztecs";
  }
      public static void main(String[] args){
      Artifact Artifact1 = new Artifact();
      Artifact Artifact2 = new Artifact();
      Artifact Artifact3 = new Artifact();
      System.out.println(Artifact1.num);
      System.out.println(Artifact2.num + Artifact2.year );
      System.out.println(Artifact3.num + Artifact3.year + Artifact3.culture);
  }

}
